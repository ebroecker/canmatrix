[CommandSetNVParam]
ID= CFFAA41h // Provides access to configure non-volatile parameters.  Note that these parameters can only be set when the inverter's power stage is disabled (PowerCircuitEnabled_status in StatusBits message is 'Disabled.')
Type=Extended
DLC=8
Mux=CommandSetNVParam_MUX0 15,16 0 -m
Var=Dummy unsigned 31,16 -m /min:0.0 /max:65535.0 

[CommandSetNVParam]
DLC=8
Mux=CommandSetNVParam_MUX1 15,16 1 -m
Var=VUnder50pct unsigned 31,16 -m /u:ms /min:1.0 /max:30000.0 /d:160  // Determines the fault trip time when Line-to-line rms voltage for a phase remains under 50 percent of the nominal value specified in the CommandVF message while the inverter is running in GRID FOLLOWING mode.
Var=V50to88pct unsigned 47,16 -m /u:ms /min:1.0 /max:30000.0 /d:2000  // Determines the fault trip time when Line-to-line rms voltage for a phase remains between 50 and 88 percent of the nominal value specified in the CommandVF message while the inverter is running in GRID FOLLOWING mode.
Var=V110to120pct unsigned 63,16 -m /u:ms /min:1.0 /max:30000.0 /d:1000  // Determines the fault trip time when Line-to-line rms voltage for a phase remains between 110 and 120 percent of the nominal value specified in the CommandVF message while the inverter is running in GRID FOLLOWING mode.

[CommandSetNVParam]
DLC=8
Mux=CommandSetNVParam_MUX2 15,16 2 -m
Var=VOver120 unsigned 31,16 -m /min:1.0 /max:30000.0 /d:160  // Determines the fault trip time when Line-to-line rms voltage for a phase remains Over 120 percent of the nominal value specified in the CommandVF message while the inverter is running in GRID FOLLOWING mode.

[CommandSetNVParam]
DLC=8
Mux=CommandSetNVParam_MUX3 15,16 3 -m
Var=FreqHi unsigned 31,16 -m /u:Hz /f:0.1 /min:40.0 /max:70.0 /d:60.5  // Determines the upper bound, above which the frequency monitor will trip if the line frequency remains for the time specified in FreqHi of the LFM_ClearingTimes Mux.
Var=FreqLo unsigned 47,16 -m /u:Hz /f:0.1 /min:40.0 /max:70.0 /d:59.8  // Determines the upper bound, in which the frequency monitor will trip if the line frequency remains below this bound but above the value of FreqVeryLo for the time specified in FreqLo of the LFM_ClearingTimes Mux.
Var=FreqVeryLo unsigned 63,16 -m /u:Hz /f:0.1 /min:40.0 /max:70.0 /d:57  // Determines the upper bound, in which the frequency monitor will trip if the line frequency remains below this bound but above the value of FreqVeryLo for the time specified in FreqLo of the LFM_ClearingTimes Mux.

[CommandSetNVParam]
DLC=8
Mux=CommandSetNVParam_MUX4 15,16 4 -m
Var=FreqVeryLo unsigned 31,16 -m /u:ms /min:160.0 /max:160.0 /d:160  // Determines the time it will take for a fault trip to occur when line frequency remains below the value specified in FreqVeryLo of the LFM_Limits Mux when the inverter is in GRID FOLLOWING mode.
Var=FreqLo unsigned 47,16 -m /u:ms /min:1.0 /max:30000.0 /d:160  // Determines the time it will take for a fault trip to occur when line frequency remains between the value specified in FreqLo and FreqVeryLo of the LFM_Limits Mux when the inverter is in GRID FOLLOWING mode.
Var=FreqHi unsigned 63,16 -m /u:ms /min:160.0 /max:160.0 /d:160  // Determines the time it will take for a fault trip to occur when line frequency remains above the value specified in FreqHi of the LFM_Limits Mux when the inverter is in GRID FOLLOWING mode.

